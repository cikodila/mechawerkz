<h1>Listing uploadedfiles</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Content type</th>
    <th>Data</th>
  </tr>

<% @uploadedfiles.each do |uploadedfile| %>
  <tr>
    <td><%=h uploadedfile.name %></td>
    <td><%=h uploadedfile.content_type %></td>
    <td><%=h uploadedfile.data %></td>
    <td><%= link_to 'Show', uploadedfile %></td>
    <td><%= link_to 'Edit', edit_uploadedfile_path(uploadedfile) %></td>
    <td><%= link_to 'Destroy', uploadedfile, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New uploadedfile', new_uploadedfile_path %>