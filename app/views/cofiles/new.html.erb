<h3>New File</h3>

	
<% form_for(@cofile) do |f| %>
	<div class="tform">
		<table>
  			<%= f.error_messages %>
			<%=render :partial => 'cofile_number', :locals => {:f => f} %>
  			<%=render :partial => 'form', :locals => {:f => f} %> 
		</table>
	</div>
  	<p><%= f.submit 'Create' %></p>

<% end %>

<%= link_to 'Back', cofiles_path %>